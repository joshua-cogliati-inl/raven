<?xml version="1.0" ?>
<Simulation verbosity="debug">
  <TestInfo>
    <name>test_loaddingpattern_GA-SIM3</name>
    <author>khang</author>
    <created>2022-07-12</created>
    <classesTested>Models.Code.Simulate3-GA</classesTested>
    <description>
       An example of using the the GA and SIM3 to generate the optimized loadding pattern
    </description>
    <revisions>
      <revision author="khang" date="2020-07-12">Test created</revision>
    </revisions>
  </TestInfo>
  <RunInfo>
    <WorkingDir>Simulate_RAVEN-GA</WorkingDir>
    <Sequence>sampleGA-Simulate, print</Sequence>
    <batchSize>1</batchSize>
  </RunInfo>

  <Files>
    <Input name="simulatedata_input" type="simulatedata">sim3-param.xml</Input>
    <Input name="simulateperturb_input" type="perturb">sim3-perturb.xml</Input>
    <Input name="input" type="input">input.inp</Input>
  </Files>

  <Models>
    <Code name="MySimulate" subType="Simulate">
      <executable>simulate3</executable>
      <sequence>simulate</sequence>
    </Code>
  </Models>

  <Distributions>
    <UniformDiscrete name='FA_dist'>
      <lowerBound>0</lowerBound>
      <upperBound>4</upperBound>
      <strategy>withReplacement</strategy>
    </UniformDiscrete>
  </Distributions>
  
  <Optimizers>
    <GeneticAlgorithm name="GAopt">
      <samplerInit>
       <limit>5</limit>
        <initialSeed>42</initialSeed>
        <writeSteps>every</writeSteps>
		<type>max</type>
      </samplerInit>

      <GAparams>
        <populationSize>5</populationSize>
        <parentSelection>rouletteWheel</parentSelection>
        <reproduction>
          <crossover type="onePointCrossover">
            <crossoverProb>0.75</crossoverProb>
          </crossover>
          <mutation type="swapMutator">
            <mutationProb>0.9</mutationProb>
          </mutation>
        </reproduction>
        <fitness type="feasibleFirst"></fitness>
        <survivorSelection>fitnessBased</survivorSelection>
      </GAparams>

      <convergence>
        <AHDp>0.0</AHDp>
      </convergence>

	  <variable name="loc1">
        <distribution>FA_dist</distribution>
	  </variable>
      <variable name="loc2">
        <distribution>FA_dist</distribution>
      </variable>
      <variable name="loc3">
        <distribution>FA_dist</distribution>
      </variable>
      <variable name="loc4">
        <distribution>FA_dist</distribution>
      </variable>
      <variable name="loc5">
        <distribution>FA_dist</distribution>
      </variable>
      <variable name="loc6">
        <distribution>FA_dist</distribution>
      </variable>
      <variable name="loc7">
        <distribution>FA_dist</distribution>
      </variable>
      <variable name="loc8">
        <distribution>FA_dist</distribution>
      </variable>
      <variable name="loc9">
        <distribution>FA_dist</distribution>
      </variable>
      <variable name="loc10">
        <distribution>FA_dist</distribution>
      </variable>
      <variable name="loc11">
        <distribution>FA_dist</distribution>
      </variable>
      <variable name="loc12">
        <distribution>FA_dist</distribution>
      </variable>
      <variable name="loc13">
        <distribution>FA_dist</distribution>
      </variable>
      <variable name="loc14">
        <distribution>FA_dist</distribution>
      </variable>
      <variable name="loc15">
        <distribution>FA_dist</distribution>
      </variable>
      <variable name="loc16">
        <distribution>FA_dist</distribution>
      </variable>
      <variable name="loc17">
        <distribution>FA_dist</distribution>
      </variable>
      <variable name="loc18">
        <distribution>FA_dist</distribution>
      </variable>
      <variable name="loc19">
        <distribution>FA_dist</distribution>
      </variable>
      <variable name="loc20">
        <distribution>FA_dist</distribution>
      </variable>
      <variable name="loc22">
        <distribution>FA_dist</distribution>
      </variable>
      <variable name="loc23">
        <distribution>FA_dist</distribution>
      </variable>
      <variable name="loc24">
        <distribution>FA_dist</distribution>
      </variable>
      <variable name="loc25">
        <distribution>FA_dist</distribution>
      </variable>
      <variable name="loc28">
        <distribution>FA_dist</distribution>
      </variable>
      <variable name="loc29">
        <distribution>FA_dist</distribution>
      </variable>
	  <constant name="loc21">5.0</constant>
	<constant name="loc26">5.0</constant>
	<constant name="loc27">5.0</constant>
	<constant name="loc30">5.0</constant>
	<constant name="loc31">5.0</constant>
	<constant name="loc32">5.0</constant>
	<constant name="loc33">5.0</constant>
	<constant name="loc34">5.0</constant>
	<constant name="loc35">5.0</constant>

      <objective>target</objective>
      <TargetEvaluation class="DataObjects" type="PointSet">optOut</TargetEvaluation>
    <Sampler class="Samplers" type="MonteCarlo">MC_samp</Sampler>
    </GeneticAlgorithm>
  </Optimizers>
  <Samplers>
    <MonteCarlo name="MC_samp">
      <samplerInit>
        <limit>5</limit>
        <initialSeed>20021986</initialSeed>
      </samplerInit>
	  <variable name="loc1">
        <distribution>FA_dist</distribution>
	  </variable>
    <variable name="loc2">
      <distribution>FA_dist</distribution>
    </variable>
    <variable name="loc3">
      <distribution>FA_dist</distribution>
    </variable>
    <variable name="loc4">
      <distribution>FA_dist</distribution>
    </variable>
    <variable name="loc5">
      <distribution>FA_dist</distribution>
    </variable>
    <variable name="loc6">
      <distribution>FA_dist</distribution>
    </variable>
    <variable name="loc7">
      <distribution>FA_dist</distribution>
    </variable>
    <variable name="loc8">
      <distribution>FA_dist</distribution>
    </variable>
    <variable name="loc9">
      <distribution>FA_dist</distribution>
    </variable>
    <variable name="loc10">
      <distribution>FA_dist</distribution>
    </variable>
    <variable name="loc11">
      <distribution>FA_dist</distribution>
    </variable>
    <variable name="loc12">
      <distribution>FA_dist</distribution>
    </variable>
    <variable name="loc13">
      <distribution>FA_dist</distribution>
    </variable>
    <variable name="loc14">
      <distribution>FA_dist</distribution>
    </variable>
    <variable name="loc15">
      <distribution>FA_dist</distribution>
    </variable>
    <variable name="loc16">
      <distribution>FA_dist</distribution>
    </variable>
    <variable name="loc17">
      <distribution>FA_dist</distribution>
    </variable>
    <variable name="loc18">
      <distribution>FA_dist</distribution>
    </variable>
    <variable name="loc19">
      <distribution>FA_dist</distribution>
    </variable>
    <variable name="loc20">
      <distribution>FA_dist</distribution>
    </variable>
    <variable name="loc22">
      <distribution>FA_dist</distribution>
    </variable>
    <variable name="loc23">
      <distribution>FA_dist</distribution>
    </variable>
    <variable name="loc24">
      <distribution>FA_dist</distribution>
    </variable>
    <variable name="loc25">
      <distribution>FA_dist</distribution>
    </variable>
    <variable name="loc28">
      <distribution>FA_dist</distribution>
    </variable>
    <variable name="loc29">
      <distribution>FA_dist</distribution>
	</variable>
	<constant name="loc21">5.0</constant>
	<constant name="loc26">5.0</constant>
	<constant name="loc27">5.0</constant>
	<constant name="loc30">5.0</constant>
	<constant name="loc31">5.0</constant>
	<constant name="loc32">5.0</constant>
	<constant name="loc33">5.0</constant>
	<constant name="loc34">5.0</constant>
	<constant name="loc35">5.0</constant>
    </MonteCarlo>
  </Samplers>

  <Steps>
      <MultiRun name="sampleGA-Simulate" re-seeding="2286">
      <Input class="Files" type="simulatedata">simulatedata_input</Input>
      <Input class="Files" type="perturb">simulateperturb_input</Input>
      <Input class="Files" type="input">input</Input>
      <Model          class="Models" type="Code">MySimulate</Model>
      <Optimizer      class="Optimizers"  type="GeneticAlgorithm" >GAopt</Optimizer>
      <SolutionExport class="DataObjects" type="PointSet">opt_export</SolutionExport>
      <Output class="DataObjects" type="PointSet">optOut</Output>
      <Output class="OutStreams" type="Print">opt_export</Output>
    </MultiRun>
	<IOStep name="print">
      <Input class="DataObjects" type="PointSet">opt_export</Input>
      <Input class="DataObjects" type="PointSet">optOut</Input>
      <Output class="OutStreams" type="Print">opt_export</Output>
      <Output class="OutStreams" type="Print">optOut</Output>
    </IOStep>
  </Steps>
  <Functions>
    <External file="Simulate_RAVEN-GA/genmap.py" name="gen_pattern">
      <variables>
	  loc1, loc2, loc3, loc4, loc5, loc6, loc7,
	  loc8, loc9, loc10, loc11, loc12, loc13, loc14,
	  loc15, loc16, loc17, loc18, loc19, loc20,
	  loc22, loc23, loc24, loc25, loc28,
	  loc29
	  </variables>
    </External>
	<External file="Simulate_RAVEN-GA/constraints/FuelReloadCaseConstraints.py" name="impConstr1">
      <variables>
	  loc1, loc2, loc3, loc4, loc5, loc6, loc7,
	  loc8, loc9, loc10, loc11, loc12, loc13, loc14,
	  loc15, loc16, loc17, loc18, loc19, loc20,
	  loc22, loc23, loc24, loc25, loc28,
	  loc29
	  </variables>
    </External>
	<External file="Simulate_RAVEN-GA/constraints/FuelReloadCaseConstraints.py" name="impConstr2">
      <variables>
	  loc1, loc2, loc3, loc4, loc5, loc6, loc7,
	  loc8, loc9, loc10, loc11, loc12, loc13, loc14,
	  loc15, loc16, loc17, loc18, loc19, loc20,
	  loc22, loc23, loc24, loc25, loc28,
	  loc29
	  </variables>
    </External>
	<External file="Simulate_RAVEN-GA/constraints/FuelReloadCaseConstraints.py" name="impConstr3">
      <variables>
	  loc1, loc2, loc3, loc4, loc5, loc6, loc7,
	  loc8, loc9, loc10, loc11, loc12, loc13, loc14,
	  loc15, loc16, loc17, loc18, loc19, loc20,
	  loc22, loc23, loc24, loc25, loc28,
	  loc29
	  </variables>
    </External>
	<External file="Simulate_RAVEN-GA/constraints/FuelReloadCaseConstraints.py" name="impConstr4">
      <variables>
	  loc1, loc2, loc3, loc4, loc5, loc6, loc7,
	  loc8, loc9, loc10, loc11, loc12, loc13, loc14,
	  loc15, loc16, loc17, loc18, loc19, loc20,
	  loc22, loc23, loc24, loc25, loc28,
	  loc29
	  </variables>
    </External>
	<External file="Simulate_RAVEN-GA/constraints/FuelReloadCaseConstraints.py" name="impConstr5">
      <variables>
	  loc1, loc2, loc3, loc4, loc5, loc6, loc7,
	  loc8, loc9, loc10, loc11, loc12, loc13, loc14,
	  loc15, loc16, loc17, loc18, loc19, loc20,
	  loc22, loc23, loc24, loc25, loc28,
	  loc29
	  </variables>
    </External>
  </Functions>
  <DataObjects>
    <PointSet name="optOut">
      <Input>
	  loc1, loc2, loc3, loc4, loc5, loc6, loc7,
	  loc8, loc9, loc10, loc11, loc12, loc13, loc14,
	  loc15, loc16, loc17, loc18, loc19, loc20,
	  loc22, loc23, loc24, loc25, loc28,
	  loc29
	  </Input>
      <Output>target, MaxEFPD, pin_peaking, max_boron, MaxFDH, batchId</Output>
    </PointSet>
	<PointSet name="opt_export">
    <Input>trajID</Input>
    <Output>
		  loc1, loc2, loc3, loc4, loc5, loc6, loc7,
	    loc8, loc9, loc10, loc11, loc12, loc13, loc14,
	    loc15, loc16, loc17, loc18, loc19, loc20,
	    loc22, loc23, loc24, loc25, loc28,
	    loc29, target,iteration,age, rejectReason,batchId,fitness,accepted
		</Output>
  </PointSet>
  </DataObjects>
  <OutStreams>
    <Print name="optOut">
      <type>csv</type>
      <source>optOut</source>
    </Print>
    <Print name="opt_export">
      <type>csv</type>
      <source>opt_export</source>
      <clusterLabel>trajID</clusterLabel>
    </Print>
  </OutStreams>

</Simulation>
